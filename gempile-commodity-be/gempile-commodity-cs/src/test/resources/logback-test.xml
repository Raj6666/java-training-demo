<configuration>

    <!-- Output to console -->
    <appender name="console_log_appender" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg %n</pattern>
        </encoder>
    </appender>

    <!-- level and appender -->
    <root level="INFO">
        <appender-ref ref="console_log_appender"/>
    </root>

</configuration> 